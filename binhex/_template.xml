<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <RemoveFromCA>true</RemoveFromCA>
  <TemplateURL>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/appnamelower.xml</TemplateURL>
  <Beta>false</Beta>
  <Category>appcategory:</Category>
  <Name>binhex-appnamelower</Name>
  <Overview>[b]Description[/b]&#xD;
appdescription-paragraph1&#xD;
&#xD;
appdescription paragraph2&#xD;
&#xD;
[b]Configuration[/b]&#xD;
[b]/config[/b] This is the container path to your configuration files, database and logs etc.&#xD;
[b]/media[/b] This is the container path to your media files, i.e. movies, tv, music, pictures etc.&#xD;
[b]/data[/b] This is the container path to your downloaded files, user created content etc.[br]&#xD;

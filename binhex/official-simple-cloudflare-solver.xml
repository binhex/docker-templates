<?xml version="1.0"?>
<Container version="2">
    <Name>binhex-official-simple-cloudflare-solver</Name>
	<Beta>false</Beta>
    <Category>Network:Other Other:</Category>
    <Repository>ghcr.io/nlevee/simple-cloudflare-solver:sha-a3cdb8b</Repository>
    <Registry>https://github.com/nlevee/simple-cloudflare-solver/pkgs/container/simple-cloudflare-solver</Registry>
    <Network>bridge</Network>
    <Privileged>false</Privileged>
    <PrivilegedReq>false</PrivilegedReq>
    <ExtraParams></ExtraParams>
    <Shell>bash</Shell>
    <GitHub>https://github.com/nlevee/simple-cloudflare-solver</GitHub>
    <Support>https://github.com/nlevee/simple-cloudflare-solver/issues</Support>
    <Project>https://github.com/nlevee/simple-cloudflare-solver</Project>
